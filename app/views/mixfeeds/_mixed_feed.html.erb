<% feeds.each do |share| %>
  <% if share.is_a?(Hashie::Mash) %>
    <%= render 'instagram_shares/share', instagram: share %>
  <% elsif share.is_a?(Twitter::Tweet) %>
    <%= render 'twitter_shares/tweet', tweet: share %>
  <% else %>
    <%= render 'facebook_shares/share', share: share.deep_symbolize_keys %>
  <% end %>
<% end %>
