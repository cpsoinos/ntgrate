<% @feeds.each do |share| %>
  <% if share.is_a?(Hashie::Mash) %>
    $("<%= escape_javascript(render 'instagram_shares/share', instagram: share) %>").appendTo(".feed-container");
  <% elsif share.is_a?(Twitter::Tweet) %>
    $("<%= escape_javascript(render 'twitter_shares/tweet', tweet: share) %>").appendTo(".feed-container");
  <% else %>
    $("<%= escape_javascript(render 'facebook_shares/share', share: share.deep_symbolize_keys) %>").appendTo(".feed-container");
  <% end %>
<% end %>
